#include<stdio.h>
int main(){
int m=29;
printf("Address of m: %",&m);
printf("\nvalue of m: %",m);
int *ab=&m;
printf("Now ab is assigned with the address of m.");
printf("Address of ab: %",ab);
printf("value of ab: %",*ab);
printf("The value of m assigned to 34 now.");
*ab=34;
printf("Address of ab: %",ab);
printf("value of ab: %",*ab);
printf("The pointer variable ab is assigned with the value 7 now.");
*ab=7;
printf("Address of m: %",&m);
printf("value of m: %",m);
return 0;
}
